<p id="notice"><%= notice %></p>

<h1>Service Types <span style="padding-left: 15%;"><%= link_to 'New Service Type', new_service_type_path %></span></h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Created At</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @service_types.each do |service_type| %>
      <tr>
        <td><%= service_type.name %></td>
        <td><%= date_format(service_type.created_at) %></td>
        <td><%= link_to 'Show', service_type %></td>
        <td><%= link_to 'Edit', edit_service_type_path(service_type) %></td>
        <td><%= link_to 'Destroy', service_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


